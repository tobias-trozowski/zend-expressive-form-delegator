{
    "name": "tobias/zend-expressive-form-delegator",
    "description": "FormElementManager delegator to pickup forms, fieldsets and elements from config.",
    "type": "library",
    "homepage": "https://github.com/tobias-trozowski/zend-expressive-form-delegator",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Trozowski",
            "email": "tobias.trozowski@trozowski-it.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/tobias-trozowski/zend-expressive-form-delegator/issues",
        "wiki": "https://github.com/tobias-trozowski/zend-expressive-form-delegator/wiki",
        "source": "https://github.com/tobias-trozowski/zend-expressive-form-delegator/tree/master"
    },
    "prefer-stable": true,
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zend-servicemanager": "^2.7.6 || ^3.0.0",
        "zendframework/zend-form": "^2.8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6 || ^5.2.10",
        "zendframework/zend-coding-standard": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Tobias\\Expressive\\Form\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TobiasTest\\Expressive\\Form\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v"
    }
}
